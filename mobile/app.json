{
  "expo": {
    "name": "No Limit Flix",
    "slug": "no-limit-flix",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nolimitflix.app",
      "infoPlist": {
        "LSApplicationQueriesSchemes": [
          "vlc",
          "infuse"
        ]
      }
    },
    "android": {
      "package": "com.nolimitflix.app",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-web-browser",
      "expo-video"
    ],
    "extra": {
      "eas": {
        "projectId": "ae07b6bc-80f3-44ef-ae7f-00a7c8b152f0"
      }
    }
  }
}
